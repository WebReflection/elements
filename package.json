{
  "name": "@webreflection/elements",
  "version": "0.1.7",
  "description": "HTML & SVG Custom Elements",
  "main": "index.js",
  "module": "index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && rollup -c rollup.js && npm run types",
    "types": "rm -rf types && tsc --allowJs --checkJs --lib esnext,dom --moduleResolution nodenext --module NodeNext --target esnext -d --emitDeclarationOnly --outDir ./types ./index.js ./auto.js ./native.js"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "types": "./types/index.d.ts"
    },
    "./auto": {
      "import": "./dist/auto.js",
      "types": "./types/auto.d.ts"
    },
    "./native": {
      "import": "./native.js",
      "types": "./types/native.d.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*",
    "types/*",
    "auto.js",
    "index.js",
    "native.js",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "custom elements",
    "web components",
    "native",
    "extends",
    "is"
  ],
  "author": "Andrea Giammarchi",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "nonchalance": "^0.6.1",
    "qsa-observer": "^3.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.57.1",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/elements.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/elements/issues"
  },
  "homepage": "https://github.com/WebReflection/elements#readme"
}
